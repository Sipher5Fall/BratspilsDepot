@model List<Spil>

    <div>
    @for (int i = 0; i < Model.Count; i++)
    {

        <div class = "Item">

            <div class = "img">
                <img src ="@Model[i].SpilBilledStiFuld"/>
            </div>

            <div class="Title">
                @Model[i].SpilNavn
               
            </div>

            <div class="Pris">
                @Model[i].SpilPris
            </div>

            <div class="Antal">
               @Model[i].SpilAntal

            </div>

        </div>




    }
</div>