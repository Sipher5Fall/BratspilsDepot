<!DOCTYPE html>
@model List<string[]> //forvent List<string[]> type

<style>
    div.card-columns {
        width:100%;
        overflow:auto;
    }

    div.card-columns div.card {
        width:33%;
        height:300px;
        float:left;
    }
</style>


<body>
<div class="card-columns">
@foreach (string[] spil in Model)
{
    <div class="card">
        <div class="card-img" >
            <div class="card-body">
                <h1 class="card-title">@spil[0]</h1>
                <p class="card-info"><div class="card-details" style="float:left">@($"Pris: {spil[2]}")</div><div class="card-details" style="float:right">@($"Kategori: {spil[3]}")</div></p>
            </div>
            <img src="@spil[4]" />
        </div>
    </div>
}
</div>

</body>


