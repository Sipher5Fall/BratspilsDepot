<!DOCTYPE html>
@model List<string[]> //forvent List<string[]> type

<style>
    div.card-columns {
        width:100%;
        overflow:auto;
    }

    div.card-columns div.card {
        width:33%;
        height:300px;
        float:left;
    }
</style>


<body>
<div class="card-columns">
@foreach (string[] spil in Model)
{
    <div class="card">
        <div class="card-img" >
            <div class="card-body">
                <h2 class="card-title" style="color:darkcyan">@spil[0]</h2>
                <h5 class="card-info"> @($"Pris: {spil[2]} Kategori: {spil[3]}")</h5>
                            
                                <form action="/Home/PutIkurven" metode ="get">
                                 <input type ="hidden" name="Id" value="@spil[1]"/> 
                                <button>Tilføj</button>

                                </form>
                          
            </div>
            <img src="@spil[4]" />
        </div>
    </div>
}
</div>
</body>
   


